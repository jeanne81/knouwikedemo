<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>

<% include ../partials/nav.ejs %>

<div class="container-fluid">
  <h2>Database Results</h2>
  
  <div class="row">
    <div class="col-sm-12">
      <ul class="list-group">
          <% results.forEach(function(r) { %>
              <li class="list-group-item"><%= r.id %> - <%= r.name %></li>
          <% }); %>
      </ul>
    </div>  
  </div>

</div>

</body>
</html>
